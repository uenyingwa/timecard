<%= form_with(model: time_entry, url: time_entry_path(time_entry), method: :patch, class: "task-form") do |f|  %>
  <div>
    <ul class="list-group">
      <% time_entry.errors.full_messages.each do |error| %>
        <li class="list-group-item list-group-item-danger"><%= error %></li>
      <% end %>
    </ul>
  </div>
  <div class="mb-3">
    <%= f.label :Start_time, class: "form-label" %>
    <%= f.text_field :created_at, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :End_time, class: "form-label" %>
    <%= f.text_field :clock_out, class: "form-control", rows: "3" %>
  </div>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
